{
  "name": "Stick It! Extension",
  "description": "Simple To-Do notes extension",
  "version": "1.1",
  "author": "Michelle Arquiza",
  "manifest_version": 3,
  "icons": {
    "16": "/images/sticky-16.png",
    "48": "/images/sticky-48.png",
    "128": "/images/sticky-128.png"
  },
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides" : {
    "newtab": "index.html"
  }
}